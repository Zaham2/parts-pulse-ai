{
  "expo": {
    "name": "Parts Pulse AI",
    "slug": "parts-pulse-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon.ico",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/placeholder.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.partspulseai.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/favicon.ico",
        "backgroundColor": "#ffffff"
      },
      "package": "com.partspulseai.app"
    },
    "web": {
      "favicon": "./public/favicon.ico",
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "plugins": [
      "nativewind"
    ],
    "scheme": "parts-pulse-ai"
  }
}